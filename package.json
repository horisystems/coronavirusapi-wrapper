{
  "name": "coronavirusapi-wrapper",
  "version": "2.2.7",
  "description": "A wrapper to work with the Coronavirus API.",
  "main": "dist/index.js",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "build": "rimraf dist && npm run build:esm6 && npm run build:cjs && npm run build:esm5 && npm run build:umd && npm run build:umd:min && npm run delete:esm5",
    "build:esm6": "tsc --module es6 --target es6",
    "build:esm5": "tsc --module es2015 --target es5 --outDir dist/esm5",
    "delete:esm5": "rimraf dist/esm5",
    "build:cjs": "tsc --module commonjs --target es5 --outDir dist/commonjs",
    "build:umd": "rollup dist/esm5/index.js --format umd --name coronavirusapi-wrapper --o dist/umd/coronavirusapi-wrapper.js",
    "build:umd:min": "cd dist/umd && uglifyjs --compress --mangle --comments --output coronavirusapi-wrapper.min.js -- coronavirusapi-wrapper.js",
    "test": "jest --config jest.config.json --coverage",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "commit": "git commit -s -m april2022 endpoint",
    "tag": "git tag -s -a v2.2.7 -m v2.2.7 && git push origin --tags",
    "delete:local:tag": "git tag -d v2.2.7",
    "delete:remote:tag": "git push origin --delete v2.2.7",
    "publish": "npm publish"
  },
  "files": [
    "lib",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moatsystems/coronavirusapi-wrapper.git"
  },
  "keywords": [
    "coronavirus",
    "covid19",
    "api",
    "wrapper",
    "js",
    "tdd",
    "library"
  ],
  "author": "Finbarrs Oketunji <f@finbarrs.eu> (https://finbarrs.eu/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moatsystems/coronavirusapi-wrapper/issues"
  },
  "homepage": "https://github.com/moatsystems/coronavirusapi-wrapper#readme",
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/node": "^14.10.2",
    "jest": "^26.4.2",
    "node-fetch": "^2.6.1",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.27.0",
    "ts-jest": "^26.3.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.2",
    "uglify-js": "^3.10.4"
  }
}
